{
	"name": "directus-extension-streaming-video-player",
	"description": "Play HLS and MPEG-DASH adaptive video streams and standard video files with this enhanced video player interface",
	"icon": "play_circle",
	"version": "1.0.6",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-interface",
		"video",
		"player",
		"hls",
		"streaming"
	],
	"type": "module",
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/domdus/directus-extension-streaming-video-player.git"
	},
	"bugs": {
		"url": "https://github.com/domdus/directus-extension-streaming-video-player/issues"
	},
	"homepage": "https://github.com/domdus/directus-extension-streaming-video-player#readme",
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^17.0.0",
		"vue": "^3.3.7",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"hls.js": "^1.4.12",
		"dashjs": "^4.7.4",
		"crypto-js": "^4.2.0"
	}
