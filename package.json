{
	"name": "directus-extension-streaming-video-player",
	"description": "Play HLS (m3u8) video streams and standard video files with this enhanced video player interface",
	"icon": "play_circle",
  "version": "1.0.0",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-interface",
		"video",
		"player",
		"hls",
		"streaming"
	],
	"type": "module",
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
